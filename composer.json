{
    "name": "hjp1011/yii2-auth",
    "description": "Extension for Yii2 Framework to work with  auththorization",
    "keywords": ["yii2", "auth", "authorization", "yiiframe", "yiiframework"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/hjp1011/yii2-auth/issues",
        "source": "https://github.com/hjp1011/yii2-auth"
    },
    "authors": [
        {
            "name": "hjp1011"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13"
    },
    "autoload": {
        "psr-4": {
            "yiiframe\\auth\\": "./src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
